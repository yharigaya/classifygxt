	pmcid = {PMC4716687},
	keywords = {Humans, Quantitative Trait Loci, Linkage Disequilibrium},
	pages = {216--224},
	file = {Full Text:/Users/yurikoharigaya/Zotero/storage/56B4P76M/Davis et al. - 2016 - An Efficient Multiple-Testing Adjustment for eQTL .pdf:application/pdf},
}

